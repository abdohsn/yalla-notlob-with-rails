			    <div class="panel-heading">
			        Latest Orders
			    </div>
			    <div class="panel-body">
			        <div class="row">
			            <div class="col-lg-12">
			            <% @orders.each do |order| %>
							<b>
							<%= link_to order_orders_details_path(order) do %>
							<%= order.orderType %> on <%= order.created_at %> </b>
							<% end %>
							<br>
						<% end %>  
			            </div>
			        </div>
			    </div>
			</div>
		</div>


	</div>
</div>

